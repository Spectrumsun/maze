    "jest-transform-css": "^2.1.0",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "jest-transform-css"
    },
    "setupFiles": [
      "<rootDir>/src/tests/testConfig.js"
